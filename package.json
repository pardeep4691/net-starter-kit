{
  "name": "net-starter-kit",
  "version": "1.0.0",
  "description": "This is the starter kit to use for everyone who loves node typescript express",
  "main": "src/main.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=local ts-node ./src/main.ts",
    "prod": "NODE_ENV=prod ts-node ./src/main.ts",
    "format": "prettier --write ./src/",
    "lint": "eslint ./src/ --ext .ts --fix",
    "pre:fixed": "npm run format && npm run lint",
    "prepare": "husky install",
    "build": "tsc"
  },
  "keywords": [
    "starter-kit",
    "boilerplate"
  ],
  "author": "Pardeep Kumar",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.12",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.30.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0"
  }
}
